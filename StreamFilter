import java.util.ArrayList;


public class Main {
    public static void main(String[] args) {

        ArrayList<Integer> numbers = new ArrayList();
        numbers.add(3);
        numbers.add(2);
        numbers.add(6);
        numbers.add(123);
        numbers.add(39);

        numbers.stream().filter(n -> n % 1 == 0)
                .forEach(System.out::println);
    }
}
